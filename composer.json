{
    "name": "icambridge/http",
    "description": "A custom library for building an evented http server.",
    "keywords": ["http"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzle/parser": "~3.0",
        "react/socket": "0.4.*",
        "react/stream": "0.4.*",
        "evenement/evenement": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Icambridge\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icambridge\\Tests\\Http\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
